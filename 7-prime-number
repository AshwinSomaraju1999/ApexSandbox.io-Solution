public Boolean isPrime(Integer num) {
    boolean result;
    Integer count=0;
    if(num>1){
        for(Integer i=1;i<=num;i++){
            if(math.mod(num,i)==0){
                count = count+1;
            }
        }
    }
    if(count>2){
        result = false;
    }
    else{
        result = true;
    }
    return result;
}
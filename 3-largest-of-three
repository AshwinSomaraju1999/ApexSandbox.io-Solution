public static Integer findLargest(Integer num1, Integer num2, Integer num3) {
    integer largestNum;
    if(num1!=null && num2!=null && num3!=null){
        largestNum = num1;
        if(num2>largestNum){
            largestNum = num2;
        }
        if(num3>largestNum){
            largestNum = num3;
            }
    }
    return largestNum;
}